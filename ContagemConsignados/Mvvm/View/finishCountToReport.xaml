<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="ContagemConsignados.Mvvm.View.finishCountToReport"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="finishCountToReport">
    <VerticalStackLayout>
        <Entry
            Margin="10"
            Placeholder="Hospital"
            Text="{Binding Hospital}" />

        <Button Command="{Binding UpdateCountCommand}" Text="FINALIZAR" />
    </VerticalStackLayout>
</ContentPage>